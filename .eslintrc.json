{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "no-tabs": ["error", { "allowIndentationTabs": true }],
        "indent": ["error", "tab"],
        "max-len": ["error", { "code": 120 }],
        "eqeqeq": "off"
    }
}
